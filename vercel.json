{
  "rewrites": [
    {
      "source": "/header/:match*",
      "destination": "https://header-single-spa-mfe-example.vercel.app/:match*"
    },
    {
      "source": "/home/:match*",
      "destination": "https://home-single-spa-mfe-example.vercel.app/:match*"
    },
    {
      "source": "/app-search/:match*",
      "destination": "https://search-single-spa-mfe-example.vercel.app/:match*"
    },
    {
      "source": "/app-settings/:match*",
      "destination": "https://settings-single-spa-mfe-example.vercel.app/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
